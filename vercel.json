{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/assets/(.*)",
            "dest": "/frontend/assets/$1"
        },
        {
            "src": "/images/(.*)",
            "dest": "/frontend/images/$1"
        },
        {
            "src": "/js/(.*)",
            "dest": "/frontend/js/$1"
        },
        {
            "src": "/css/(.*)",
            "dest": "/frontend/css/$1"
        },
        {
            "src": "/api/(.*)",
            "dest": "server.js"
        },
        {
            "src": "/(.*)",
            "dest": "server.js"
        }
    ]
}